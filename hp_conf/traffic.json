{
    "dataset": "Seoul city traffic data",
    "model": "RNN variants",
    "config": { 
    },
    "hyperparams": {
        "data_type": {
            "type": "int",
            "value_type": "preordered",
            "range": [20, 21, 22, 23, 24, 25, 30, 31, 32, 33, 34, 35]
        },
        "img_size": {
            "type": "float",
            "value_type": "preordered",
            "range": [0.2, 0.5, 1, 2, 5]
        },
        "img_depth": {
            "type": "int",
            "value_type": "discrete",
            "range": [0, 0]
        },
        "forecasting_horizon": {
            "type": "int",
            "value_type": "preordered",
            "range": [1, 3, 6, 12]
        },                
        "seq_len": {
            "type": "int",
            "value_type": "preordered",
            "range": [6, 12, 24]
        },
        "model_type": {
            "type": "str",
            "value_type": "categorical",
            "range": ["lstm", "cnnlstm", "ndlstm", "3dcnn"]
        },

        "batch_size": {
            "type": "int",
            "value_type": "discrete",
            "range": [64, 64]
        },
        "max_epoch": {
            "type": "int",
            "value_type": "discrete",
            "range": [30, 30]
        },

        "optimizer": {
            "type": "str",
            "value_type": "categorical",
            "range": ["adam"]
        },
        "opt_learningrate": {
            "type": "float",
            "value_type": "continuous",
            "range": [-4, -1],
            "power_of": 10
        },        
        "batchnorm_on": {
            "type": "int",
            "value_type": "preordered",
            "range": [0, 1]
        },
        "dropout_on": {
            "type": "int",
            "value_type": "preordered",
            "range": [0, 1]
        }, 
        "loss_func": {
            "type": "str",
            "value_type": "categorical",
            "range": ["mean_squared_error"]
        },  
        "earlystop_on": {
            "type": "int",
            "value_type": "preordered",
            "range": [0, 1]
        },
        
        "n_conv_layers": {
            "type": "int",
            "value_type": "discrete",
            "range": [1, 5]
        },
        "conv1_depth": {
            "type": "int",
            "value_type": "preordered",
            "range": [4, 8, 16, 32, 64]
        },
        "conv2_depth": {
            "type": "int",
            "value_type": "preordered",
            "range": [4, 8, 16, 32, 64]
        },
        "conv3_depth": {
            "type": "int",
            "value_type": "preordered",
            "range": [4, 8, 16, 32, 64]
        },
        "conv4_depth": {
            "type": "int",
            "value_type": "preordered",
            "range": [4, 8, 16, 32, 64]
        },
        "conv5_depth": {
            "type": "int",
            "value_type": "preordered",
            "range": [4, 8, 16, 32, 64]
        },
        "conv_fc_units": {
            "type": "int",
            "value_type": "discrete",
            "range": [128, 512]
        }, 
        "act_conv": {
            "type": "str",
            "value_type": "categorical",
            "range": ["relu"]
        },
        "pooling_on": {
            "type": "int",
            "value_type": "preordered",
            "range": [0, 1]
        },
        "pooling_size": {
            "type": "int",
            "value_type": "preordered",
            "range": [2, 3]
        },
        "conv_filter_size": {
            "type": "int",
            "value_type": "preordered",
            "range": [3]
        },        
        
        "n_lstm_layers": {
            "type": "int",
            "value_type": "preordered",
            "range": [1]
        },
        "lstm_units": {
            "type": "int",
            "value_type": "preordered",
            "range": [128]
        },
        "act_lstm": {
            "type": "str",
            "value_type": "categorical",
            "range": ["tanh"]
        },
        "lstm_fc_units": {
            "type": "int",
            "value_type": "preordered",
            "range": [20]
        },
        
        "n_fc_layers": {
            "type": "int",
            "value_type": "preordered",
            "range": [20]
        },        
        "last_fc_units": {
            "type": "int",
            "value_type": "preordered",
            "range": [20]
        },  
        "temp_type": {
            "type": "str",
            "value_type": "categorical",
            "range": ["default"]
        },
        "traj_opt": {
            "type": "str",
            "value_type": "categorical",
            "range": ["all"]
        },
        "normalization_opt": {
            "type": "str",
            "value_type": "categorical",
            "range": ["raw"]
        }
    },
    "param_order" : ["data_type", "img_size", "img_depth", "forecasting_horizon",
        "seq_len", "model_type", "batch_size", 
        "max_epoch", "optimizer", "opt_learningrate",
        "batchnorm_on", "dropout_on", "loss_func", "earlystop_on",
        "n_conv_layers", "conv1_depth", "conv2_depth", "conv3_depth", "conv4_depth", "conv5_depth",
        "conv_fc_units", "act_conv", "pooling_on", "pooling_size", "conv_filter_size",
        "n_lstm_layers",  "lstm_units", "act_lstm", "lstm_fc_units", "n_fc_layers",
        "last_fc_units", "temp_type", "traj_opt", "normalization_opt"
    ]
}